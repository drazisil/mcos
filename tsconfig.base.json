{
    "compilerOptions": {
        "esModuleInterop": true,
        "skipLibCheck": true,
        "target": "ES2022",
        "incremental": true,
        "verbatimModuleSyntax": true,
        "resolveJsonModule": true,
        "moduleDetection": "force",
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "moduleResolution": "Bundler",
        "module": "ES2022",
        "lib": ["ES2022"],
        "declaration": true,
        "isolatedModules": true,
        "inlineSourceMap": true
    },
    "files": []
}
