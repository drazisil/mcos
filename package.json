{
  "name": "mco-server",
  "version": "3.0.0-beta1",
  "description": "a server",
  "main": "index.js",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "AUTHORS"
  ],
  "scripts": {
    "prestart": "yarn run build",
    "start:dev": "node -r ts-node/register src/app.ts",
    "start": "node --trace-warnings dist/src/app.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "build": "tsc",
    "lint": "eslint src/**/*.ts && prettier --write src test",
    "test": "yarn run lint-test",
    "lint-test": "yarn run lint && yarn run test:only",
    "test:only": "jest",
    "precommit": "yarn run lint-test",
    "postinstall": "yarn audit && mkdir -p db"
  },
  "author": "Drazi Crendraven <drazisil@hey.com>",
  "license": "MPL-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:drazisil/mco-server.git"
  },
  "dependencies": {
    "@drazisil/mco-logger": "4.1.10",
    "sqlite3": "5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "@tsconfig/node14": "1.0.1",
    "@types/node": "16.7.10",
    "@types/pg": "8.6.1",
    "@types/sinon": "10.0.2",
    "@types/sqlite3": "3.1.7",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.2.0",
    "prettier": "2.4.1",
    "prettier-eslint": "13.0.0",
    "sinon": "11.1.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.6",
    "testdouble": "3.16.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  }
}
