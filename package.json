{
  "name": "mcos",
  "version": "3.1.0-beta1",
  "description": "a server",
  "main": "packages/bin/app.ts",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "AUTHORS"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "check:license": "npm licenses audit --summary --config=licenses.config.js",
    "start": "node dist/bin/app.js | ./node_modules/.bin/pino-pretty",
    "dev": "ts-node src/bin/app.ts | ./node_modules/.bin/pino-pretty",
    "build": "tsc --build --verbose",
    "watch": "tsc --noEmit --watch src/bin/app.ts",
    "clean": "rm -rf dist",
    "build:clean": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Drazi Crendraven <drazisil@hey.com>",
  "license": "MPL-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:drazisil/mcos.git"
  },
  "dependencies": {
    "pino": "next"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@tsconfig/node16": "1.0.2",
    "@types/chai": "4.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "babel-jest": "27.5.1",
    "chai": "4.3.6",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "mocha": "9.2.1",
    "pino-pretty": "7.5.1",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "source-map-support": "0.5.21",
    "sqlite": "4.0.23",
    "sqlite3": "4.2.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "nyc": {
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "useSpawnWrap": true
  },
  "volta": {
    "node": "16.14.0",
    "npm": "8.5.1"
  }
}
