{
  "name": "mco-server",
  "version": "3.0.0-beta1",
  "description": "a server",
  "main": "index.js",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "AUTHORS"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint src/**/*.ts && prettier --write src test",
    "test": "npm run lint-test",
    "lint-test": "npm run lint && npm run test:only",
    "test:only": "jest --testPathPattern=test/ --coverage --coverageDirectory=coverage",
    "precommit": "npm run lint-test",
    "postinstall": "mkdir -p db"
  },
  "author": "Drazi Crendraven <drazisil@hey.com>",
  "license": "MPL-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:drazisil/mco-server.git"
  },
  "dependencies": {
    "@drazisil/mco-logger": "4.1.7",
    "sqlite3": "5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@tsconfig/node14": "1.0.1",
    "@types/node": "14.17.5",
    "@types/pg": "8.6.1",
    "@types/sinon": "10.0.2",
    "@types/sqlite3": "3.1.7",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "prettier-eslint": "12.0.0",
    "sinon": "11.1.1",
    "source-map-support": "0.5.19",
    "supertest": "6.1.3",
    "testdouble": "3.16.1",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5"
  }
}
